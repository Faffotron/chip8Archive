###########################################
#
#  Expedition
#
#  Made for Nokia 3310 Jam 3, 2021
#  the theme was "On / Off".
#
###########################################

:alias menu-index   vd
:alias menu-key     vc
:alias room-index   vb
:alias script-acc   va
:alias script-pc    v9
:alias sfx-register v8
:alias frames       v7
# v0-v5 and ve-vf reserved as temporaries

:macro indirect LABEL {
	0xF0 0x00 : LABEL 0x00 0x00 # i := long NNNN
}
:macro no-op {
	v0 := v0
}

:const WIDTH  84
:const HEIGHT 48
:const TOP     8
:const LEFT   22
:calc  BOTTOM { TOP  + HEIGHT }
:calc  RIGHT  { LEFT + WIDTH  }

:macro nokia-init {
	hires
	plane 2
	i  := block-16
	v1 := BOTTOM
	loop
		sprite v0 v1 0
		v0 += 16
		if v0 != 128 then
	again
	v1 := TOP
	loop
		i  := block-16
		v0 := RIGHT
		sprite v0 v1 0
		v0 += 16
		sprite v0 v1 0
		v0 += 16
		i  := block-12
		sprite v0 v1 0
		v1 += 16
		if v1 != BOTTOM then
	again
	plane 1
}

: frame
	vf := 1
	jump wait-raw
: wait
	vf := 4
: wait-raw
	delay := vf
	loop
		vf := delay
		if vf != 0 then
	again
;

###########################################
#
#  Sound Effects
#
###########################################

: play-sound
	load sfx-register - sfx-register
	vf := 1
	i += vf
	audio
	buzzer := sfx-register
;

: play-success
	:macro note SOUND LEN {
		i := long SOUND
		play-sound
		vf := LEN
		wait-raw
	}
	note sound-low-note 7
	vf := 2
	wait-raw
	note sound-low-note 7
	vf := 2
	wait-raw
	note sound-high-note 14
;

:macro sfx SOUND {
	i := long SOUND
	play-sound
}

###########################################
#
#  Text Rendering
#
###########################################

:const STR_END     0xFF
:const STR_POS     0xFE
:const STR_SPACE   0xF9
:const CHAR_HEIGHT 8

:alias string-offset v3
:alias cursor-x      v4
:alias cursor-y      v5

: print-op
	if v0 == STR_POS begin
		string-offset += 2
		cursor-x := v1
		cursor-y := v2
		return
	end
	if v0 != STR_SPACE begin
		i  := long nokia-font
		v1 := v0
		v0 += v0
		v0 += v1 # *3
		i  += v0 # *3
		i  += v0 # *6
		i  += v0 # *9
		sprite cursor-x cursor-y CHAR_HEIGHT
		vf := 8
		i  += vf
		load v0
		cursor-x += v0
	else
		cursor-x += 3
	end
;
: print-string
	i := print-string-read
	save v1
	string-offset := 0
	cursor-x      := 0
	cursor-y      := 0
	loop
		indirect print-string-read
		i += string-offset
		load v2
		while v0 != STR_END
		string-offset += 1
		print-op
	again
;

: dialog-pause
	# debounced key input, to prevent
	# missing out on dialog.
	vf := OCTO_KEY_E
	loop if vf  key then again # wait for release,
	loop if vf -key then again # wait for press,
	loop if vf  key then again # wait for release...
	i := long sound-dialog
	play-sound
	wait                       # ...plus a minimum time delay.
	clear
;

: dialog-draw-answer
	v0 := 24
	if script-acc == 0 then v0 += 48
	v1 := 46
	i  := long mask-ask
	sprite v0 v1 0
	v0 += 16
	sprite v0 v1 0
;
: dialog-ask
	# returns 0/1 in script-acc
	:unpack long string-ask
	print-string
	ve := OCTO_KEY_E
	loop if ve key then again # wait for release
	wait                      # ...plus a minimum time delay.
	script-acc := 1
	loop
		dialog-draw-answer
		ve := key
		dialog-draw-answer
		while ve != OCTO_KEY_E
		if ve == OCTO_KEY_A begin
			script-acc := 1
			sfx sound-menu
		end
		if ve == OCTO_KEY_D begin
			script-acc := 0
			sfx sound-menu
		end
	again
	ve := OCTO_KEY_E
	sfx sound-choose
	loop if ve key then again # wait for release
	clear
;


###########################################
#
#  Scripting
#
###########################################

# a simple bytecode interpreter allows us to express
# non-performance-sensitive code without taking up code RAM:

: script-exec # entrypoint; take a pointer in v0-v1
	i := script-base
	save v0 - v1
	script-acc := 0
	script-pc  := 0
: script-step
	indirect script-base
	i += script-pc
	script-pc += 1
	load v0 - v2
	jump0 script-dispatch
: script-dispatch
: do-op-end
	return
: do-op-clear
	clear
	jump script-step
: do-op-const
	script-acc := v1
	script-pc  += 1
	jump script-step
: do-op-random
	i := random-target
	save v1 - v1
	script-pc += 1
	:next random-target
	script-acc := random 0x00
	jump script-step
: do-op-inc
	script-acc += 1
	if script-acc == 0 then script-acc := 255 # saturating inc
	jump script-step
: do-op-dec
	script-acc += -1
	if script-acc == 255 then script-acc := 0 # saturating dec
	jump script-step
: do-op-not
	if script-acc == 0 begin
		script-acc := 1 # logical not
		jump script-step
	end
	script-acc := 0
	jump script-step
: do-op-load
	script-addr
	load script-acc - script-acc
	jump script-step
: do-op-save
	script-addr
	save script-acc - script-acc
	jump script-step
: do-op-table
	script-addr
	i += script-acc
	load v0 - v1
	jump script-exec
: do-op-jumpif
	if script-acc == 0 then script-pc += 2
	if script-acc == 0 then jump script-step
	# otherwise, fall through...
: do-op-jump
	script-addr
	v0 := v1
	v1 := v2
	jump script-exec
: do-op-call
	i := script-trampoline
	save v1 - v2
	script-pc += 2
	: script-trampoline 0 0
	jump script-step
: do-op-text
	clear
	script-pc += 2
	v0 := v1
	v1 := v2
	print-string
	dialog-pause
	jump script-step
: do-op-string
	script-pc += 2
	v0 := v1
	v1 := v2
	print-string
	play-success
	dialog-pause
	jump script-step
: do-op-ask
	script-pc += 2
	clear
	v0 := v1
	v1 := v2
	print-string
	dialog-ask
	jump script-step
: do-op-draw-if
	if script-acc == 0 then script-pc += 2
	if script-acc == 0 then jump script-step
	script-addr
	load v1
	sprite v0 v1 0
	jump script-step
: do-op-blit-if
	if script-acc == 0 then script-pc += 2
	if script-acc == 0 then jump script-step
	script-addr
	clear
	draw-background
	jump script-step
: do-op-travel
	script-pc += 1
	room-index := v1
	menu-first
	jump script-step
: script-addr
	i := script-addr-slot
	save v1 - v2
	script-pc += 2
	indirect script-addr-slot
;

:macro opcode OP { :byte { OP - script-dispatch } }
:macro s-end          { opcode do-op-end                   }
:macro s-clear        { opcode do-op-clear                 }
:macro s-const     NN { opcode do-op-const   :byte NN      }
:macro s-random    NN { opcode do-op-random  :byte NN      }
:macro s-inc          { opcode do-op-inc                   }
:macro s-dec          { opcode do-op-dec                   }
:macro s-not          { opcode do-op-not                   }
:macro s-load    ADDR { opcode do-op-load    :pointer ADDR }
:macro s-save    ADDR { opcode do-op-save    :pointer ADDR }
:macro s-table   ADDR { opcode do-op-tab     :pointer ADDR }
:macro s-jumpif  ADDR { opcode do-op-jumpif  :pointer ADDR }
:macro s-jump    ADDR { opcode do-op-jump    :pointer ADDR }
:macro s-call    ADDR { opcode do-op-call    :call    ADDR }
:macro s-text    ADDR { opcode do-op-text    :pointer ADDR }
:macro s-string  ADDR { opcode do-op-string  :pointer ADDR }
:macro s-ask     ADDR { opcode do-op-ask     :pointer ADDR }
:macro s-draw-if ADDR { opcode do-op-draw-if :pointer ADDR }
:macro s-blit-if ADDR { opcode do-op-blit-if :pointer ADDR }
:macro s-travel  ROOM { opcode do-op-travel  :byte ROOM    }

:macro s-flag-set   ADDR { s-const 1  s-save ADDR }
:macro s-flag-clr   ADDR { s-const 0  s-save ADDR }
:macro s-jumpunless ADDR { s-not  s-jumpif ADDR }

:macro s-text-unless ADDR {
	:calc target { HERE + 3 + 3 }
	s-jumpif target
	s-text   ADDR
}

:macro draw-delta X Y {
	:byte { X + LEFT } # x position of sprite
	:byte { Y + TOP  } # y position of sprite
}

:macro s-item-pickup IMAGE NAME {
	s-const 1
	s-blit-if IMAGE
	s-string NAME
}

###########################################
#
#  Rooms
#
###########################################

:const NO_CODE   0
:const NO_SCRIPT 0
:const NO_IMAGE  0
:const NO_FLAG   0

:macro room-code NAME BACKGROUND CODE SCRIPT {
	: NAME
	:pointer BACKGROUND # pointer to background image
	:call    CODE       # native subroutine to run after drawing background
	:pointer SCRIPT     # script to run after returning from subroutine
	:byte    0          # no menu items (this distinguishes code from non-code)
}
:macro room-begin NAME BACKGROUND SCRIPT {
	: NAME
	:pointer BACKGROUND            # pointer to background image
	:pointer NO_CODE               # this is not a 'native' room
	:pointer SCRIPT                # script to run after drawing the screen OR 0
	:calc item-count-slot { HERE } # number of menu items
	:calc item-count { 0 }
	:org { HERE + 1 }
}
:macro room-end {
	:calc after { HERE }
	:org item-count-slot
	:byte item-count
	:org after
}
:macro item-if FLAG FLAGVAL MENU SCRIPT IMAGE X Y {
	:pointer MENU             # menu string
	:pointer SCRIPT           # menu script OR 0
	:pointer IMAGE            # selection sprite OR 0
	:byte    { X + LEFT     } # x position of sprite
	:byte    { Y + TOP      } # y position of sprite
	:pointer FLAG             # address of flag OR 0
	:byte    FLAGVAL          # display this menu item iff the flag matches this value
	:calc item-count { item-count + 1 }
}
:macro item MENU SCRIPT IMAGE X Y {
	item-if NO_FLAG -1 MENU SCRIPT IMAGE X Y
}

: draw-background
	v1 := 32   # stride
	:calc x0 { LEFT }         v0 := x0
	:calc y0 { TOP + 0 * 16 } v2 := y0
	:calc y1 { TOP + 1 * 16 } v3 := y1
	:calc y2 { TOP + 2 * 16 } v4 := y2
	loop
		:macro bg-slice REG { sprite v0 REG 0  i += v1 }
		bg-slice v2
		bg-slice v3
		bg-slice v4
		v0 += 16
		:calc bg-end { x0 + 6 * 16 }
		if v0 != bg-end then
	again
;

:macro draw-arrow-left {
	:calc ax { LEFT }         v0 := ax
	:calc ay { TOP + 38 + 2 } v1 := ay
	i := long arrow-left
	sprite v0 v1 7
}
:macro draw-arrow-right {
	:calc ax { RIGHT - 4 }    v0 := ax
	:calc ay { TOP + 38 + 2 } v1 := ay
	i := long arrow-right
	sprite v0 v1 7
}

: get-room
	i := long rooms
	i += room-index
	i += room-index
	load v1
	i := room-base
	save v1
	indirect room-base
;
: get-menu
	get-room
	v0 := 7 # offset to menu entries
	i  += v0
	v0 := menu-index # *1
	v0 += v0         # *2
	v0 += v0         # *4
	v0 += v0         # *8
	i  += v0
	v0 := menu-index # *1
	v0 += v0         # *2
	v0 += menu-index # *3
	i  += v0         # 8+3=11
;
: get-menu-size
	get-room
	v0 := 6 # offset to menu size
	i += v0
	load v0
;
: get-menu-flag
	get-menu
	v0 := 8 # offset to flag info
	i += v0
	load v2
	if v0 == 0 begin
		v0 := 1 # NO_FLAG, always succeed
		return
	end
	i := flag-lookup
	save v1
	indirect flag-lookup
	load v1 - v1
	v0 := 1
	if v1 != v2 then v0 := 0 # fail on flag mismatch
;
:macro run-room-logic {
	get-room
	v0 := 2 # offset to room logic
	i += v0
	load v1
	i := code-room-trampoline
	save v1
	: code-room-trampoline 0 0
}
: run-menu-script
	get-menu
	v0 := 2 # offset to menu item script
	i += v0
	load v1
	if v0 == 0 begin
		# don't need to check the low byte
		# since scripts don't go in code space!
		:breakpoint nyi-menu-item
		no-op
	end
	jump script-exec

: draw-menu-text
	get-menu
	load v1 # no offset for menu string
	print-string
;
: draw-sel-icon
	get-menu
	v0 := 4 # offset to icon
	i  += v0
	load v3
	if v0 == 0 then return
	# don't need to check the low byte
	# since icons don't go in code space!
	i := sel-icon-base
	save v1
	indirect sel-icon-base
	sprite v2 v3 0
;

: draw-room
	clear
	get-room
	load v1 # no offset for background slot
	i := room-bg
	save v1
	indirect room-bg
	draw-background

	run-room-script

	get-menu-size
	if v0 == 0 then return
	draw-arrow-left
	draw-arrow-right
	draw-menu-text
	draw-sel-icon
;

: run-room-script
	get-room
	v0 := 4 # offset to exit/display script
	i += v0
	if v0 == 0 then return
	# don't need to check the low byte
	# since scripts don't go in code space!
	load v1
	script-exec
;

: run-room-anim
	i := long room-anims
	i += room-index
	i += room-index
	load v1
	if v0 != 0 begin
		i := anim-trampoline
		save v1
		: anim-trampoline 0 0
	end
	frames += 1
;

: menu-wait
	vf := 10
	delay := vf
	loop
		vf := OCTO_KEY_D if vf key then menu-key :=  1
		vf := OCTO_KEY_S if vf key then menu-key :=  1
		vf := OCTO_KEY_A if vf key then menu-key := -1
		vf := OCTO_KEY_W if vf key then menu-key := -1
		vf := OCTO_KEY_E if vf key then menu-key :=  2
		vf := delay
		if vf != 0 then
	again
;

: menu-first
	menu-index := 0
	loop
		get-menu-flag
		if v0 == 1 then return
		menu-index += 1 
	again

: menu-inc
	# wait for key release
	: menu-inc-debounce
	vf := OCTO_KEY_D
	if vf key then jump menu-inc-debounce
	vf := OCTO_KEY_S
	if vf key then jump menu-inc-debounce
	sfx sound-menu
	draw-menu-text
	loop
		menu-index += 1
		get-menu-size
		if menu-index == v0 then menu-index := 0
		get-menu-flag
		if v0 == 1 then jump draw-menu-text
	again

: menu-dec
	# wait for key release
	: menu-dec-debounce
	vf := OCTO_KEY_A
	if vf key then jump menu-dec-debounce
	vf := OCTO_KEY_W
	if vf key then jump menu-dec-debounce
	sfx sound-menu
	draw-menu-text
	loop
		get-menu-size
		if menu-index == 0 then menu-index := v0
		menu-index -= 1
		get-menu-flag
		if v0 == 1 then jump draw-menu-text
	again

###########################################
#
#  Special Animation
#
###########################################

: title-screen
	i := long bg-title-screen
	draw-background
	v7 := OCTO_KEY_E
	loop
		wait
		while v7 -key
		wait
		while v7 -key
		:unpack long press-space
		print-string
		wait
		while v7 -key
		wait
		while v7 -key
	again
	loop if v7 key then again
	clear
	:unpack long intro-sequence
	script-exec
;

: long-pause
	vf := 120
	delay := vf
	loop
		vf := delay
		if vf != 0 then
	again
;

: over-eyes
	0x48 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00
	0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00
: game-over
	clear
	i := long bg-the-end-0
	draw-background
	long-pause
	clear
	i := long bg-the-end-1
	draw-background
	long-pause
	clear
	i := long bg-the-end-2
	draw-background
	v0 := 29
	v1 := 43
	i  := over-eyes
	frames := 0
	loop
		wait
		if frames == 54 then sprite v0 v1 0
		frames += 1
		vf := 63
		frames &= vf
	again

: bubble-0
	0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x01 0x00 0x08 0x00
	0x00 0x20 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00
: bubble-1
	0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x01 0x00 0x08 0x00
	0x00 0x20 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00
: bubble-2
	0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x0C 0x00 0x01 0x20 0x08 0x00
	0x02 0x20 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00
: bubble-3
	0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x0C 0x10 0x01 0x20 0x00 0x00
	0x12 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00
: anim-counter
	i := bubble-0
	v0 := frames
	vf := 0x3
	v0 &= vf
	v0 <<= v0 # *2
	v0 <<= v0 # *4
	v0 <<= v0 # *8
	v0 <<= v0 # *16
	i  += v0  #
	i  += v0  # *32
	v0 := 25
	v1 := 16
	sprite v0 v1 0	
;

: closet-eyes draw-delta 25 18
	0x00 0x00 0x44 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00
	0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00
: anim-closet
	v2 := frames
	vf := 31
	v2 &= vf
	i := closet-eyes
	load v1
	if v2 == 18 then sprite v0 v1 0
	if v2 == 19 then sprite v0 v1 0
;

: computer-garbage
	i := 0x200
	v2 := random 0xFF
	i += v2
	i += v2
	i += v2
	i += v2
	sprite v0 v1 0
;
: anim-computer
	v0 := 48
	v1 := 18
	computer-garbage
	v0 += 16
	computer-garbage
;

###########################################
#
#  Entrypoint
#
###########################################

: main
	nokia-init
	title-screen
	room-index := 0
	menu-index := 0
	loop
		frames := 0
		draw-room
		get-menu-size
		if v0 == 0 begin
			# debounce incoming keypresses:
			vf := OCTO_KEY_E
			loop if vf key then again
			run-room-logic
			run-room-script
			# note: this type of room is never actually
			# used in the game; it ended up being unnecessary.
		else
			menu-key := 0
			loop
				menu-wait
				draw-sel-icon
				run-room-anim
				if menu-key == -1 then menu-dec
				if menu-key ==  1 then menu-inc
				while menu-key != 2
				menu-key := 0
				menu-wait
				draw-sel-icon
				run-room-anim
			again
			# debounce action key
			sfx sound-choose
			vf := OCTO_KEY_E
			loop if vf key then again
			run-menu-script
		end
	again

###########################################
#
#  Engine Assets
#
###########################################

:calc FREE { 4096 - HERE }
: FREE_CODE_BYTES
:byte { FREE >> 8 }
:byte { FREE }
:monitor FREE_CODE_BYTES "%2i"
:monitor menu-index "%i"
:monitor room-index "%i"

: block-16
	0xFF 0xFF 0xFF 0xFF 0xFF 0xFF 0xFF 0xFF
	0xFF 0xFF 0xFF 0xFF 0xFF 0xFF 0xFF 0xFF
	0xFF 0xFF 0xFF 0xFF 0xFF 0xFF 0xFF 0xFF
	0xFF 0xFF 0xFF 0xFF 0xFF 0xFF 0xFF 0xFF
: block-12
	0xFF 0xF0 0xFF 0xF0 0xFF 0xF0 0xFF 0xF0
	0xFF 0xF0 0xFF 0xF0 0xFF 0xF0 0xFF 0xF0
	0xFF 0xF0 0xFF 0xF0 0xFF 0xF0 0xFF 0xF0
	0xFF 0xF0 0xFF 0xF0 0xFF 0xF0 0xFF 0xF0

: sound-dialog
	1  0x02 0x21 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00
: sound-choose
	1  0x02 0x21 0x00 0x08 0x66 0x00 0xD8 0x01 0xB0 0x20 0x10 0x10 0x00 0x00 0x00 0x00
: sound-menu
	1  0xFF 0x00 0xFF 0x00 0xFF 0x00 0xFF 0x00 0xFF 0x00 0xFF 0x00 0xFF 0x00 0xFF 0x00
: sound-low-note
	5  0xFF 0xC0 0x1F 0xF0 0x07 0xFC 0x01 0xFF 0x00 0x7F 0xC0 0x1F 0xF8 0x03 0xFE 0x00
: sound-high-note
	7  0xFF 0x01 0xFC 0x07 0xF0 0x1F 0xE0 0x3F 0x80 0xFE 0x03 0xF8 0x07 0xF0 0x1F 0xC0

: nokia-font
	0x70 0xD8 0xD8 0xD8 0xF8 0xD8 0xD8 0x00 0x06 # A
	0xF0 0xD8 0xF0 0xD8 0xD8 0xD8 0xF0 0x00 0x06 # B
	0x78 0xC0 0xC0 0xC0 0xC0 0xC0 0x78 0x00 0x06 # C
	0xF0 0xD8 0xD8 0xD8 0xD8 0xD8 0xF0 0x00 0x06 # D
	0xF8 0xC0 0xF0 0xC0 0xC0 0xC0 0xF8 0x00 0x06 # E
	0xF8 0xC0 0xF0 0xC0 0xC0 0xC0 0xC0 0x00 0x06 # F
	0x70 0xC0 0xC0 0xD8 0xD8 0xD8 0x78 0x00 0x06 # G
	0xD8 0xD8 0xF8 0xD8 0xD8 0xD8 0xD8 0x00 0x06 # H
